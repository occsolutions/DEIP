{
  "name": "ts-express-api-mongo",
  "version": "1.0.0",
  "description": "A starting point for Node.js REST API",
  "license": "UNLICENSED",
  "scripts": {
    "start": "env-cmd yarn build && yarn serve",
    "build": "env-cmd yarn build-ts && yarn tslint",
    "serve": "env-cmd node --inspect dist/server.js",
    "up": "env-cmd -f .env.deip.server.prod node dist/server.js",
    "up:qa": "env-cmd -f .env.deip.server.qa node dist/server.js",
    "watch-node": "env-cmd nodemon dist/server.js",
    "watch": "env-cmd concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc --skipLibCheck",
    "watch-ts": "tsc -w --skipLibCheck",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "yarn build && yarn watch-debug",
    "serve-debug": "env-cmd nodemon --inspect dist/server.js",
    "watch-debug": "env-cmd concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\""
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/passport-jwt": "^3.0.1",
    "@types/superagent": "^4.1.15",
    "applicationinsights": "^1.7.6",
    "async": "^3.2.4",
    "azure-storage": "^2.10.3",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "compression": "^1.7.1",
    "connect-mongo": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "errorhandler": "^1.5.0",
    "express": "^4.18.2",
    "express-jwt": "^5.3.0",
    "express-session": "^1.17.3",
    "express-validator": "^4.3.0",
    "extend": ">=3.0.2",
    "indicative": "5.0.8",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "lusca": "^1.5.2",
    "mongoose": "^6.9.0",
    "multer": "^1.4.2",
    "papaparse": "^5.3.0",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.0",
    "slugify": "^1.6.5",
    "superagent": "^5.2.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.18",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.33",
    "@types/connect-mongo": "0.0.34",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.0.35",
    "@types/express-jwt": "0.0.38",
    "@types/express-session": "^1.15.6",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.91",
    "@types/lusca": "^1.5.0",
    "@types/mongodb": "^2.2.17",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.50",
    "@types/nodemailer": "^1.3.32",
    "@types/papaparse": "^5.3.7",
    "@types/request": "^2.0.9",
    "@types/shelljs": "^0.7.7",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.7.7",
    "supertest": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "typescript": "^4.9.4"
  }
}
